$light     = 'FuturaLight'
$regular   = 'FuturaRegular'
$medium    = 'FuturaMedium'
$bold      = 'FuturaBold'
$color     = #149DCC
$default   = #61CE70
flexCenter()
	display flex
	display -webkit-flex
	align-items center
flexStart()
	display flex
	display -webkit-flex
	align-items start
flexEnd()
	display flex
	display -webkit-flex
	align-items flex-end
transitionAll()
	-webkit-transition all .3s linear
	-moz-transition all .3s linear
	-ms-transition all .3s linear
	-o-transition all .3s linear
	transition all .3s linear
transitionOpacity()
	-webkit-transition opacity .3s linear
	-moz-transition opacity .3s linear
	-ms-transition opacity .3s linear
	-o-transition opacity .3s linear
	transition opacity .3s linear
transitionWidth()
	-webkit-transition width .3s linear
	-moz-transition width .3s linear
	-ms-transition width .3s linear
	-o-transition width .3s linear
	transition width .3s linear
transitionMaxWidth()
	-webkit-transition max-width .3s linear
	-moz-transition max-width .3s linear
	-ms-transition max-width .3s linear
	-o-transition max-width .3s linear
	transition max-width .3s linear
transitionBgColor()
	-webkit-transition background-color .3s linear
	-moz-transition background-color .3s linear
	-ms-transition background-color .3s linear
	-o-transition background-color .3s linear
	transition background-color .3s linear
transitionTransform()
	-webkit-transition transform .3s linear
	-moz-transition transform .3s linear
	-ms-transition transform .3s linear
	-o-transition transform .3s linear
	transition transform .3s linear
absolute()
	position absolute
	top 0
	left 0
	right 0
	bottom 0
relative()
	position relative
	top 0
	left 0
	right 0
	bottom 0
body
	margin 0
	padding 0
	overflow hidden
	overflow-y scroll
	font-family $regular
	color #000000
	background-color #FFFFFF
img
	width 100%
p
	margin 0
	font-size 18px
label
	font-weight 100
strong
	font-family $medium
.jm-section
	position relative
	padding 40px 20px
.jm-white
	background-color #FFFFFF
.jm-grey
	background-color #F1EFF2
.jm-input
	text-align left
	margin 10px 0
	&.text-right
		text-align right !important
	label
		font-size 20px
		line-height 24px
		font-family $medium
	.form-control
		font-size 20px
		line-height 24px
	textarea
		resize none
	.mdl-button
		width 100%
		max-width 200px
		margin 10px 0
.jm-title
	text-align center
	margin 10px 0
	position relative
	h2
		font-size 36px
		line-height 40px
		font-family $bold
		color #000000
		width 100%
		max-width 250px
		margin auto
		background-color #FFFFFF
	hr
		position absolute 
		top 20px
		border 1.5px solid #000000
		z-index -1
		width 100%
		margin 0
.jm-flex
	flexCenter()
	justify-content center
.jm-flex--start
	flexStart()
	justify-content center
.jm-height
	height 100vh
	.jm-fondo
		height 100vh
		&:BEFORE
			height 100vh
.jm-relative
	position relative
/*NAVBAR*/
.navbar-default
	transitionAll()
	border none
	border-radius 0
	position fixed
	width 100%
	z-index 999
	margin 0
	background-color #FFFFFF
	top 0
	&.navbarcolor
		box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
		-webkit-box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
		-moz-box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
	&.navbar-grey
		box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
		-webkit-box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
		-moz-box-shadow 0 3px 1px -2px rgba(0,0,0,.2)
		.navbar-nav
			&>li
				&>a
					color #000000 !important
		.navbar-brand
			span 
				color #000000 !important
	.navbar-nav
		&>li
			&>a
				color #000000
				font-size 20px
				text-align center
				&:hover
					color #FFFFFF
	.navbar-brand
		flexCenter()
		color #000000 !important
		font-size 20px
		font-family $bold
		&>img
			max-width 40px
		span 
			margin-left 15px
			font-size 24px
.js-header
	transitionAll()
	position fixed
	top 0
	width 100%
	z-index 5
	padding 5px 25px
	color #FFFFFF
	height 75px
	background-color rgba(0, 0, 0, .4)
	.js-header--container
		flexCenter()
		width 100%
		max-width 1150px
		margin auto
		height 100%
	img
		width 100%
		max-width 200px
	.js-header--left
		flexCenter()
		width 100%
		max-width 300px
		justify-content flex-start
	.js-header--right
		flexCenter()
		width 100%
		justify-content flex-end
		text-align right
.jm-container
	transitionMaxWidth()
	height 100%
	width 100%
	max-width 1150px
	margin auto
.js-home
	height 500px
.jm-pulsera	
	height 260px
.js-input
	padding 7px 20px
	height 70px
	margin 0
	border 1px solid rgba(0,0,0,0.15)
	background-color #FFFFFF
	margin-bottom -1px
	color #333333
	margin 5px 0
	&.js-input--default
		width 100%
		max-width 400px
		margin 20px auto
	&.js-date
		padding 5px 20px
		.js-icon
			width 100%
			max-width 36px
			text-align center
	&.js-file
		height 100%
	&.js-radio
		height 100%
		.mdl-radio
			width 100%
			.mdl-radio__inner-circle
				background $default
			&.is-checked
				.mdl-radio__outer-circle
					border 2px solid $default
			.mdl-radio__ripple-container 
				.mdl-ripple
					background $default
	&.js-select
		padding 0
		.bootstrap-select
			width 100% !important
			height 100%;
			.dropdown-toggle
				padding 14px 24px
				border-radius 0
				box-shadow none
				background-color transparent
				height 100%
				border none
				font-size 16px
				font-family $medium
				color #000000
			.dropdown-menu
				background-color rgba(248, 248, 248, 0.95)
				border-radius 0
				margin 0 
				padding 0
				&.open
					margin 0
					padding 0
					li
						a
							padding 6px 20px
							font-size 18px
							&:hover
								background-color rgba(221,221,221,0.5)
	label
		font-family $light
		color #666666
		cursor pointer
		margin-bottom 0
		font-weight 100
		font-size 14px
	input
		display block
		height 36px
		width 100%
		border none
		font-size 16px
		&:focus
			outline 0
.js-select
	&.js-standar
		margin 5px 0
		.bootstrap-select
			width 100% !important
			.btn-default
				background-color #FFFFFF
				color #757575
				height 46px
				border-radius 0	
.js-contact-label
	flexCenter()
	flex-wrap wrap
	.js-input-label
		width 100%
		max-width 120px
.js-label
	height 145px
	margin 5px 0
	span
		font-family $bold
		a
			color #008FD3 !important
	.mdl-checkbox__label
		font-size 14px
		color #000000
		line-height 18px
		font-family $regular
		font-weight 100
.jm-button
	background-color $default !important
	text-decoration none !important
	text-transform none
	font-size 16px
	font-family $medium
	color #FFFFFF !important
	box-shadow none !important
	&.jm-button--default
		background-color transparent !important
		border 2px solid #FFFFFF
		width 100%
		max-width 200px
		margin-top 20px
	&.jm-button--login
		background-color $default !important
		width 100%
	&.jm-button--black
		background-color transparent !important
		border 2px solid #000000
		color #000000 !important
		width 100%
		max-width 200px
		margin-top 20px
.mdl-radio__inner-circle
	background #4983B6
.mdl-checkbox
	&.is-checked 
		.mdl-checkbox__tick-outline
			background #4983B6
.mdl-checkbox__ripple-container 
	&.mdl-ripple
		background #4983B6
.mdl-radio__ripple-container 
	.mdl-ripple
		background #4983B6
.mdl-checkbox
	&.is-checked 
		.mdl-checkbox__box-outline
			border 2px solid #4983B6
.mdl-radio
	&.is-checked 
		.mdl-radio__outer-circle
			border 2px solid #4983B6
.js-checkbox
	.mdl-checkbox
		max-width 250px
	.mdl-checkbox__label
		font-size 15px
		line-height 18px
		font-weight 100
	.mdl-checkbox__box-outline
		border 2px solid #FFFFFF
.jm-fondo
	absolute()
	background url(../img/fondo/fondo_home.jpg) no-repeat center center
	background-size cover !important
	width 100%
	height 500px
	z-index -1
	&.time
		background url(../img/fondo/fondo_time.jpg) no-repeat center center
	&.about
		background url(../img/fondo/fondo_about.JPG) no-repeat center center
	&.story1
		background url(../img/fondo/fondo_story1.JPG) no-repeat top center
	&.story2
		background url(../img/fondo/fondo_story2.JPG) no-repeat bottom center
	&.pulsera
		background url(../img/logo/benitan_pulsera.jpg) no-repeat center center
		height 260px
		&:BEFORE
			height 260px
	&:BEFORE
		absolute()
		content ''
		background-color #000000
		z-index 0
		height 500px
		opacity .2
footer
	background-color #A7A9AC
	color #FFFFFF
	padding 20px
	p
		font-size 16px
		line-height 20px			
/*CLASES PROYECTO*/
.table-responsive
	border none
.jm-redes
	margin-top 20px
	ul
		padding 0
		margin 0
		li
			list-style none
			display inline-block
			padding 5px 20px
			a
				i 
					color #FFFFFF
.jm-autor
	color #D14B3C
	font-family $medium
	margin-bottom 15px
	padding 5px 20px
.jm-contenido
	padding 20px
	text-align center
	p
		font-size 24px
		line-height 30px
		font-family $medium
	img 
		max-width 250px
		margin-bottom 20px
	span 
		font-size 20px
		line-height 24px
		display block
		margin-top 10px
.jm-subtitle
	text-align center
	p
		font-size 24px
		line-height 30px
		font-family $medium
.js-contenido
	text-align center
	color #FFFFFF
	max-width 680px
	h2
		font-size 60px
		line-height 60px
		font-family $medium
	p
		font-size 32px
		line-height 34px
		margin-bottom 10px
	small
		font-size 18px
.collection
	text-align center
	max-width 300px
	margin 20px auto
	.mdl-button
		text-align left
		margin 20px 0
		padding 0
		.jm-dollar
			display inline-block
			height 100%
			background-color #000000
			color #FFFFFF
			margin-right 30px
			padding 0 8px
	p
		font-size 20px
		line-height 24px
.jm-grid-2
	display grid
	grid-template-columns repeat(auto-fit,minmax(200px,1fr))
	grid-gap 10px
.jm-grid-4
	display grid
	grid-template-columns repeat(auto-fit,minmax(200px,1fr))
	grid-gap 50px
	.jm-icon
		width 100%
		max-width 200px
		margin auto
		text-align center
		p
			font-size 20px
			line-height 24px
.jm-tabs
	&>li
		&>a
			color #818181
		&.active
			&>a
				color #000000
				font-family $medium
/*MATERIAL DESIGN*/
.mdl-card
.mdl-card__supporting-text
	width 100%
/*SCROLL*/
::-webkit-scrollbar
	width 5px
	height 5px
::-webkit-scrollbar-track
	background-color transparent
	border-radius 10px
::-webkit-scrollbar-thumb
	background-color #AAAAAA
	border-radius 10px
@media (min-width: 768px)
	.navbar-default
		background-color transparent
		.navbar-nav
			&>li
				&>a
					color #FFFFFF !important
					font-size 16px
		.navbar-brand
			color #FFFFFF !important
		&.navbarcolor
			background-color #FFFFFF
			.navbar-nav
				&>li
					&>a
						color #000000 !important
			.navbar-brand
				color #000000 !important
@media (max-width: 600px)
	#home
		margin-top 50px
	.jm-fondo
		background url(../img/fondo/fondo_responsive.jpeg) no-repeat center center
		&.time
			background-position left
	.js-contenido
		padding 20px
		h2
			font-size 28px
			line-height 32px
	.js-header
		padding 5px 15px
		.js-header--right
			p
				font-size 16px
				line-height 16px
	.jm-input
		.mdl-button
			max-width 100%
	.dataTables_info
	.dataTables_paginate
		margin 20px 0 !important
			